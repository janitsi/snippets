<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="408.0" prefWidth="594.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="comboboxfx.MainController">
   <children>
      <ComboBox fx:id="comboBox" layoutX="25.0" layoutY="56.0" onAction="#comboChanged" prefHeight="25.0" prefWidth="197.0" promptText="Select Name" />
      <Label fx:id="nameLabel" layoutX="25.0" layoutY="22.0" prefHeight="25.0" prefWidth="197.0" />
      <Button fx:id="button" layoutX="25.0" layoutY="89.0" mnemonicParsing="false" onAction="#ButtonAction" text="Add more names" />
      <ListView fx:id="listView" layoutX="25.0" layoutY="134.0" prefHeight="200.0" prefWidth="548.0" />
   </children>
</AnchorPane>
